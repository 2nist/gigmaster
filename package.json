{
  "name": "rockstar-react",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup-assets": "node scripts/setupAssetDirectories.js",
    "validate-assets": "node scripts/validateAssets.js",
    "create-placeholders": "node --input-type=module scripts/createPlaceholderAssets.js",
    "generate-all-placeholders": "node scripts/generateAllAvatarPlaceholders.js",
    "avatar:gen": "node tools/avatar-gen/scripts/generate.js",
    "avatar:gen:alpha": "node tools/avatar-gen/scripts/generate.js --alpha",
    "avatar:sync": "node scripts/syncAvatarGenAssets.js",
    "avatar:sync:alpha": "node scripts/syncAvatarGenAssets.js --alpha",
    "avatar:gen:sync": "npm run avatar:gen && npm run avatar:sync",
    "avatar:gen:alpha:sync": "npm run avatar:gen:alpha && npm run avatar:sync:alpha",
    "avatar:manifest": "node tools/build-avatar-manifest.js",
    "avatar:meta:suggest": "node tools/suggest-avatar-metadata.js",
    "avatar:capture-poc": "node tools/capture-avatar-poc.js"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "midi-parser-js": "^4.0.4",
    "phaser": "^3.90.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tone": "^15.1.22"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.23",
    "babel-jest": "^30.2.0",
    "canvas": "^3.2.1",
    "daisyui": "^3.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "playwright": "^1.58.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "vite": "^5.3.4"
  }
}
